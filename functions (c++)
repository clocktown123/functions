#include <iostream>
using namespace std;


void printHello(int thing[]);

int main() {

	int coordinates[4];
	cout << "enter x1, y1, x2, y2" << endl;
	for (int i = 0; i < 4; i++) {
		cin >> coordinates[i];

	}

	printHello(coordinates);

}

void printHello(int thing[]) {
	cout << (thing[0] + thing[2]) / 2 << "," << (thing[1] + thing[3]) / 2 << endl;
		
}
